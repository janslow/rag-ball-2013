{
  "release-misc": {
    "expand": true,
    "cwd": "<%= site.staging %>",
    "src": ["**", "!**/*.{html,css,js,png,jpg,gif,svg}"],
    "dest": "<%= site.release %>"
  },
  "release-html": {
    "expand": true,
    "cwd": "<%= site.staging %>",
    "src": ["**/*.html"],
    "dest": "<%= site.release %>"
  }
}